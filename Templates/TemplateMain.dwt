	<script>
		let left = document.getElementById("ban-l");
		let right = document.getElementById("ban-r");
		let main = document.getElementById("ban-main");
		let content= document.getElementById("ban-content");
		let btl = document.getElementById("ban-btl");
		let btr = document.getElementById("ban-btr");
		window.addEventListener("scroll",function(){
			let value = window.scrollY;
			left.style.right = value/3 + 'px';
			right.style.left = value/3 + 'px';
			main.style.top = value*1.5 + 'px';
			content.style.top= value*0.25 + 'px';
			btl.style.right = value/6 + 'px';
			btr.style.left = value/6 + 'px';
		})
